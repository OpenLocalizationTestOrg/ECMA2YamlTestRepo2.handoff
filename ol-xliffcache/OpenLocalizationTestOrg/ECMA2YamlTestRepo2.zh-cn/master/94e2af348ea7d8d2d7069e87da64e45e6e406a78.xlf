        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies the starting position that the system uses to arrange minimized windows.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>This enumeration is used by the &lt;xref:System.Windows.Forms.SystemInformation.ArrangeStartingPosition%2A&gt; property of the &lt;xref:System.Windows.Forms.SystemInformation&gt; class.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Starts at the lower-left corner of the screen, which is the default position.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>To be added.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Starts at the lower-right corner of the screen.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>To be added.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Hides minimized windows by moving them off the visible area of the screen.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>To be added.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Starts at the upper-left corner of the screen.</source>
