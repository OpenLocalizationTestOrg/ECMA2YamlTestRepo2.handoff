ring text);\n    parameters:\n    - id: text\n      type: System.String\n      description: The text to search, which is the body of the HTTP response.\n    return:\n      type: System.Object\n      description: An object representing a Web method return value.\n  overload: System.Web.Services.Protocols.PatternMatcher.Match*\n  exceptions: []\n  platform:\n  - net462\nreferences:\n- uid: System.Object\n  isExternal: false\n  name: System.Object\n- uid: System.Web.Services.Protocols.PatternMatcher.#ctor(System.Type)\n  parent: System.Web.Services.Protocols.PatternMatcher\n  isExternal: false\n  name: PatternMatcher(Type)\n  nameWithType: PatternMatcher.PatternMatcher(Type)\n  fullName: System.Web.Services.Protocols.PatternMatcher.PatternMatcher(Type)\n- uid: System.Type\n  parent: System\n  isExternal: true\n  name: Type\n  nameWithType: Type\n  fullName: System.Type\n- uid: System.Web.Services.Protocols.PatternMatcher.Match(System.String)\n  parent: System.Web.Services.Protocols.PatternMatcher\n  isExternal: false\n  name: Match(String)\n  nameWithType: PatternMatcher.Match(String)\n  fullName: System.Web.Services.Protocols.PatternMatcher.Match(String)\n- uid: System.String\n  parent: System\n  isExternal: true\n  name: String\n  nameWithType: String\n  fullName: System.String\n- uid: System.Web.Services.Protocols.PatternMatcher.#ctor*\n  parent: System.Web.Services.Protocols.PatternMatcher\n  isExternal: false\n  name: PatternMatcher\n  nameWithType: PatternMatcher.PatternMatcher\n- uid: System.Web.Services.Protocols.PatternMatcher.Match*\n  parent: System.Web.Services.Protocols.PatternMatcher\n  isExternal: false\n  name: Match\n  nameWithType: PatternMatcher.Match\n"}