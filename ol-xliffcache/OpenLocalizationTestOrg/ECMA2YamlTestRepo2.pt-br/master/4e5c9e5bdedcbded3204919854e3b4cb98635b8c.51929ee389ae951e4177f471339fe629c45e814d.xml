ity.  \\n  \\n```  \\nstatic void CreateClientWorkflow()  \\n{  \\n   Variable<string> message = new Variable<string>(\\\"message\\\", \\\"Hello!\\\");  \\n   Variable<string> result = new Variable<string> { Name = \\\"result\\\" };  \\n  \\n   Endpoint endpoint = new Endpoint  \\n   {  \\n      AddressUri = new Uri(Microsoft.Samples.WorkflowServicesSamples.Common.Constants.ServiceBaseAddress), Binding = new BasicHttpBinding(),  \\n   };  \\n  \\n   Send requestEcho = new Send  \\n   {  \\n      ServiceContractName = XName.Get(\\\"Echo\\\", \\\"http://tempuri.org/\\\"),  \\n                Endpoint = endpoint,  \\n                OperationName = \\\"Echo\\\",  \\n      //parameters for send  \\n      Content = new SendParametersContent  \\n      {  \\n         Parameters =   \\n            {   \\n               { \\\"message\\\", new InArgument<string>(message) }  \\n            }  \\n        }  \\n    };  \\n    workflow = new CorrelationScope  \\n    {  \\n       Body = new Sequence  \\n       {  \\n          Variables = { message, result },  \\n                    Activities =  \\n                    {  \\n                        new WriteLine {  \\n                            Text = new InArgument<string>(\\\"Client is ready!\\\")  \\n                        },  \\n                        requestEcho,  \\n  \\n                        new WriteLine {  \\n                            Text = new InArgument<string>(\\\"Message sent: Hello!\\\")  \\n                        },  \\n  \\n                        new ReceiveReply  \\n                        {  \\n                            Request = requestEcho,  \\n                            //parameters for the reply  \\n                            Content = new ReceiveParametersContent                              \\n                            {  \\n                                Parameters =   \\n                                {  \\n                                    { \\\"echo\\\", new OutArgument<string>(result) }  \\n                                }  \\n                            }  \\n                        },                                                  \\n                        new WriteLine {  \\n                            Text = new InArgument<string>(env => \\\"Message received: \\\"+result.Get(env))  \\n                        }  \\n                    }  \\n                }  \\n            };  \\n        }  \\n  \\n```\"\n  syntax:\n    content: >-\n      [System.Windows.Markup.ContentProperty(\"Content\")]\n\n      public sealed class ReceiveReply : System.Activities.Activity\n  inheritance:\n  - System.Object\n  - System.Activities.Activity\n  implements: []\n  inheritedMembers:\n  - System.Activities.Activity.CacheId\n  - System.Activities.Activity.Constraints\n  - System.Activities.Activity.DisplayName\n  - System.Activities.Activity.Id\n  - System.Activities.Activity.Implementation\n  - System.Activities.Activity.ImplementationVersion\n  - System.Activities.Activity.OnCreateDynamicUpdateMap(System.Activities.DynamicUpdate.UpdateMapMetadata,System.Activities.Activity)\n  - System.Activities.Activity.ShouldSerializeDisplayName\n  - System.Activities.Activity.ToString\n  platform:\n  - net462\n- uid: System.ServiceModel.Activities.ReceiveReply.#ctor\n  id: '#ctor'\n  parent: System.ServiceModel.Activities.ReceiveReply\n  langs:\n  - csharp\n  name: ReceiveReply()\n  nameWithType: ReceiveReply.ReceiveReply()\n  fullName: System.ServiceModel.Activities.ReceiveReply.ReceiveReply()\n  type: Constructor\n  assemblies:\n  - System.ServiceModel.Activities\n  namespace: System.ServiceModel.Activities\n  summary: Initializes a new instance of the <xref href=\"System.ServiceModel.Activities.ReceiveReply\"></xref> class.\n  syntax:\n    content: public ReceiveReply ();\n    parameters: []\n  overload: System.ServiceModel.Activities.ReceiveReply.#ctor*\n  exceptions: []\n  platform:\n  - net462\n- uid: System.ServiceModel.Activities.ReceiveReply.Action\n  id: Act