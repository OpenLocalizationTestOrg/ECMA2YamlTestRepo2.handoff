        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Gets the description of the signing certificate.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The description of the signing certificate.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Gets the description URL of the signing certificate.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>The description URL of the signing certificate.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Gets the hash algorithm used to compute the signature.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>The hash algorithm used to compute the signature.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Gets the HRESULT value from verifying the signature.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>The HRESULT value from verifying the signature.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
