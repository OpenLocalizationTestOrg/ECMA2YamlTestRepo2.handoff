entModel.ToolboxItemFilter(\"System.Workflow.ComponentModel.Design.ActivitySet\", System.ComponentModel.ToolboxItemFilterType.Custom)]\n\n      [System.Obsolete(\"The System.Workflow.* types are deprecated.  Instead, please use the new types from System.Activities.*\")]\n\n      [System.Workflow.ComponentModel.Design.ActivityDesignerTheme(typeof(System.Workflow.ComponentModel.Design.ActivityDesignerTheme))]\n\n      public class ActivityDesigner : IDisposable, System.ComponentModel.Design.IDesignerFilter, System.ComponentModel.Design.IRootDesigner, System.Drawing.Design.IToolboxUser, System.Workflow.ComponentModel.Design.IPersistUIState, System.Workflow.ComponentModel.Design.IWorkflowRootDesigner\n  inheritance:\n  - System.Object\n  implements:\n  - System.ComponentModel.Design.IDesignerFilter\n  - System.ComponentModel.Design.IRootDesigner\n  - System.Drawing.Design.IToolboxUser\n  - System.IDisposable\n  - System.Workflow.ComponentModel.Design.IPersistUIState\n  - System.Workflow.ComponentModel.Design.IWorkflowRootDesigner\n  inheritedMembers: []\n  platform:\n  - net462\n- uid: System.Workflow.ComponentModel.Design.ActivityDesigner.#ctor\n  id: '#ctor'\n  parent: System.Workflow.ComponentModel.Design.ActivityDesigner\n  langs:\n  - csharp\n  name: ActivityDesigner()\n  nameWithType: ActivityDesigner.ActivityDesigner()\n  fullName: System.Workflow.ComponentModel.Design.ActivityDesigner.ActivityDesigner()\n  type: Constructor\n  assemblies:\n  - System.Workflow.ComponentModel\n  namespace: System.Workflow.ComponentModel.Design\n  summary: Initializes a new instance of the <xref href=\"System.Workflow.ComponentModel.Design.ActivityDesigner\"></xref> class.\n  syntax:\n    content: public ActivityDesigner ();\n    parameters: []\n  overload: System.Workflow.ComponentModel.Design.ActivityDesigner.#ctor*\n  exceptions: []\n  platform:\n  - net462\n- uid: System.Workflow.ComponentModel.Design.ActivityDesigner.AccessibilityObject\n  id: AccessibilityObject\n  parent: Sy