        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Defines the interface required for processing messages from simplex datagram.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>This contract has the following settings:      -   Sessions are allowed, but not required.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>-   One way messages.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Initiates the asynchronous processing of the message.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>The message to be processed.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>An &lt;xref:System.AsyncCallback&gt; delegate that is invoked when the message processing has completed.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>A user-defined state object that is passed to the callback procedure.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>An &lt;xref:System.IAsyncResult&gt; that can be used to poll or wait for results, or both; this value is also needed when invoking &lt;xref:System.ServiceModel.Routing.ISimplexDatagramRouter.EndProcessMessage*&gt;.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Finishes asynchronous execution of message processing.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>The asynchronous result returned by the call to &lt;xref:System.ServiceModel.Routing.ISimplexDatagramRouter.BeginProcessMessage*&gt;.</source>
