arent: Microsoft.Build.Tasks.ResolveAssemblyReference\n  langs:\n  - csharp\n  name: ScatterFiles\n  nameWithType: ResolveAssemblyReference.ScatterFiles\n  fullName: Microsoft.Build.Tasks.ResolveAssemblyReference.ScatterFiles\n  type: Property\n  assemblies:\n  - Microsoft.Build.Tasks.v4.0\n  namespace: Microsoft.Build.Tasks\n  summary: Gets the scatter files associated with one of the given assemblies.\n  syntax:\n    content: public Microsoft.Build.Framework.ITaskItem[] ScatterFiles { get; }\n    return:\n      type: Microsoft.Build.Framework.ITaskItem[]\n      description: The scatter files associated with one of the given assemblies.\n  overload: Microsoft.Build.Tasks.ResolveAssemblyReference.ScatterFiles*\n  exceptions: []\n  platform:\n  - net462\n- uid: Microsoft.Build.Tasks.ResolveAssemblyReference.SearchPaths\n  id: SearchPaths\n  parent: Microsoft.Build.Tasks.ResolveAssemblyReference\n  langs:\n  - csharp\n  name: SearchPaths\n  nameWithType: ResolveAssemblyReference.SearchPaths\n  fullName: Microsoft.Build.Tasks.ResolveAssemblyReference.SearchPaths\n  type: Property\n  assemblies:\n  - Microsoft.Build.Tasks.v4.0\n  namespace: Microsoft.Build.Tasks\n  summary: Specifies the directories or special locations that are searched to find the files on disk that represent the assemblies.\n  syntax:\n    content: public string[] SearchPaths { get; set; }\n    return:\n      type: System.String[]\n      description: The directories or special locations that are searched to find the files on disk that represent the assemblies.\n  overload: Microsoft.Build.Tasks.ResolveAssemblyReference.SearchPaths*\n  exceptions: []\n  platform:\n  - net462\n- uid: Microsoft.Build.Tasks.ResolveAssemblyReference.SerializationAssemblyFiles\n  id: SerializationAssemblyFiles\n  parent: Microsoft.Build.Tasks.ResolveAssemblyReference\n  langs:\n  - csharp\n  name: SerializationAssemblyFiles\n  nameWithType: ResolveAssemblyReference.SerializationAssemblyFiles\n  fullName: Microsoft.Build.Ta