ublic override string GetDesignTimeHtml ();\n    parameters: []\n    return:\n      type: System.String\n      description: The HTML used to represent the control at design time.\n  overload: System.Web.UI.Design.WebControls.TableDesigner.GetDesignTimeHtml*\n  exceptions: []\n  platform:\n  - net462\nreferences:\n- uid: System.Web.UI.Design.ControlDesigner\n  isExternal: false\n  name: System.Web.UI.Design.ControlDesigner\n- uid: System.Web.UI.Design.WebControls.TableDesigner.#ctor\n  parent: System.Web.UI.Design.WebControls.TableDesigner\n  isExternal: false\n  name: TableDesigner()\n  nameWithType: TableDesigner.TableDesigner()\n  fullName: System.Web.UI.Design.WebControls.TableDesigner.TableDesigner()\n- uid: System.Web.UI.Design.WebControls.TableDesigner.GetDesignTimeHtml\n  parent: System.Web.UI.Design.WebControls.TableDesigner\n  isExternal: false\n  name: GetDesignTimeHtml()\n  nameWithType: TableDesigner.GetDesignTimeHtml()\n  fullName: System.Web.UI.Design.WebControls.TableDesigner.GetDesignTimeHtml()\n- uid: System.String\n  parent: System\n  isExternal: true\n  name: String\n  nameWithType: String\n  fullName: System.String\n- uid: System.Web.UI.Design.WebControls.TableDesigner.#ctor*\n  parent: System.Web.UI.Design.WebControls.TableDesigner\n  isExternal: false\n  name: TableDesigner\n  nameWithType: TableDesigner.TableDesigner\n- uid: System.Web.UI.Design.WebControls.TableDesigner.GetDesignTimeHtml*\n  parent: System.Web.UI.Design.WebControls.TableDesigner\n  isExternal: false\n  name: GetDesignTimeHtml\n  nameWithType: TableDesigner.GetDesignTimeHtml\n"}