        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies the alignment of a text string relative to its layout rectangle.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>When used with the &lt;xref:System.Drawing.StringFormat.LineAlignment%2A&gt; property, this enumeration sets the vertical alignment for a drawn string.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
          <source>When used with the &lt;xref:System.Drawing.StringFormat.Alignment%2A&gt; property, this enumeration sets the horizontal alignment.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Specifies that text is aligned in the center of the layout rectangle.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>To be added.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Specifies that text is aligned far from the origin position of the layout rectangle.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>In a left-to-right layout, the far position is right.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>In a right-to-left layout, the far position is left.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>To be added.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Specifies the text be aligned near the layout.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>In a left-to-right layout, the near position is left.</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>In a right-to-left layout, the near position is right.</source>
        </trans-unit>
