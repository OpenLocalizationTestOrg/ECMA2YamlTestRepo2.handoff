        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Provides services for editing control templates at design time.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>This class cannot be inherited.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Initializes a new instance of the <bpt id="p1">&lt;xref href="System.Web.UI.Design.TemplateEditingService"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> class with the specified designer host.</source>
        </trans-unit>
