        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Represents pre-defined units of text for the purposes of navigation within a document.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>Any method using TextUnit will deprecate <ph id="ph1">`up`</ph> to the next supported TextUnit if the given TextUnit is not supported by the control.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT">
          <source>The order, from smallest unit to largest, is listed below.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT">
          <source>-   Character      -   Format      -   Word      -   Line      -   Paragraph      -   Page      -   Document</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Specifies that the text unit is one character in length.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>To be added.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Specifies that the text unit is an entire document in length.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>To be added.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Specifies that the text unit is the length of a single, common format specification, such as bold, italic, or similar.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>To be added.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
