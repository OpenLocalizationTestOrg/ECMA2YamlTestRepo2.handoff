        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies the layout of items in a list control.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT">
          <source>The RepeatLayout enumeration represents the different layout options for a list control.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Items are displayed without a table structure.</source>
        </trans-unit>
