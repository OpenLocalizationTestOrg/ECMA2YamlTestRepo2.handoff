      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies how the parent row labels of a <bpt id="p1">&lt;xref href="System.Windows.Forms.DataGrid"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> control are displayed.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Use the members of this enumeration to set the value of the &lt;xref:System.Windows.Forms.DataGrid.ParentRowsLabelStyle%2A&gt; property.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Parent rows can be displayed only if the &lt;xref:System.Windows.Forms.DataGrid&gt; contains at least one &lt;xref:System.Data.DataRelation&gt; to a second &lt;xref:System.Data.DataTable&gt;, and if the &lt;xref:System.Windows.Forms.DataGrid.AllowNavigation%2A&gt; property is set to a value that allows navigation.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Displays both the parent table and column names.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>To be added.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Displays the parent column name.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>To be added.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Display no parent row labels.</source>
        </trans-unit>
