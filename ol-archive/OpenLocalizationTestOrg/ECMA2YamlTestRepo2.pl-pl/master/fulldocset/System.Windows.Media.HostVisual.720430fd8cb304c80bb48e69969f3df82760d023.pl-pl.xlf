        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>A value of type <bpt id="p1">&lt;xref href="System.Windows.Media.PointHitTestParameters"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept>.</source>
