        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Animates an image that has time-based frames.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Displays a multiple-frame image as an animation.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The <bpt id="p1">&lt;xref href="System.Drawing.Image"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> object to animate.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>An <bpt id="p1">&lt;xref uid="langword_csharp_EventHandler" name="EventHandler" href=""&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> object that specifies the method that is called when the animation frame changes.</source>
