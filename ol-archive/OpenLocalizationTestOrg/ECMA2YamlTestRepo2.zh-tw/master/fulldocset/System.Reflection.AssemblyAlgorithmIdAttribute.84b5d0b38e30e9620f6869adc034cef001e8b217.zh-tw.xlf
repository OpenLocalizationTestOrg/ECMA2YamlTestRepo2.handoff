      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Specifies an algorithm to hash all files in an assembly.</source>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>This class cannot be inherited.</source>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>The file hash values and names in the assembly are stored in the assembly manifest.</source>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>When a file is loaded, the hash value is used to verify that a file has not been changed since the manifest was built.</source>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Initializes a new instance of the <bpt id="p1">&lt;xref href="System.Reflection.AssemblyAlgorithmIdAttribute"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> class with the specified hash algorithm, using one of the members of <bpt id="p2">&lt;xref href="System.Configuration.Assemblies.AssemblyHashAlgorithm"&gt;</bpt><ept id="p2">&lt;/xref&gt;</ept> to represent the hash algorithm.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>A member of <bpt id="p1">&lt;xref uid="langword_csharp_AssemblyHashAlgorithm" name="AssemblyHashAlgorithm" href=""&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> that represents the hash algorithm.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Initializes a new instance of the <bpt id="p1">&lt;xref href="System.Reflection.AssemblyAlgorithmIdAttribute"&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept> class with the specified hash algorithm, using an unsigned integer to represent the hash algorithm.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>An unsigned integer representing the hash algorithm.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Gets the hash algorithm of an assembly manifest's contents.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>An unsigned integer representing the assembly hash algorithm.</source>
