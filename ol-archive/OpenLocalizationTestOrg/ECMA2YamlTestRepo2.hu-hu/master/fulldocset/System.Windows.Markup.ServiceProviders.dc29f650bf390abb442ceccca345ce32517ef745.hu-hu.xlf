          <source><ph id="ph1">&lt;code&gt;serviceType&lt;/code&gt;</ph> or <ph id="ph2">&lt;code&gt;service&lt;/code&gt;</ph> is <bpt id="p1">&lt;xref uid="langword_csharp_null" name="null" href=""&gt;</bpt><ept id="p1">&lt;/xref&gt;</ept>.</source>
        </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>Attempted to add a service that already exists in the dictionary.</source>
        </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>Gets the service object of the specified type.</source>
        </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
