        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>A string that represents the pattern to match.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Gets or sets a value that represents the index of a match within a grouping.</source>
